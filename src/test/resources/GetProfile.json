

{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer"
    },
    "name": {
      "type": "string"
    },
    "email": {
      "type": "string"
    },
    "phone_number": {
      "type": "string"
    },
    "address": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "id": {
              "type": "integer"
            },
            "address": {
              "type": "string"
            }
          },
          "required": [
            "id",
            "address"
          ]
        }
      ]
    },
    "image": {
      "type": "string"
    },
    "member_code": {
      "type": "string"
    }
  },
  "required": [
    "id",
    "name",
    "email",
    "phone_number",
    "address",
    "image",
    "member_code"
  ]
}

